@model UserViewModel

<div class="form-group">
    <label asp-for="Id" for="Name" class="col-form-label"></label>
    <input asp-for="Id" class="form-control" readonly />
</div>

<div class="form-group">
    <label asp-for="FirstName" for="Age" class="col-form-label"></label>
    <input asp-for="FirstName" class="form-control" readonly />
</div>

<div class="form-group">
    <label asp-for="LastName" for="Adderss" class="col-form-label"></label>
    <input asp-for="LastName" class="form-control" readonly />
</div>

<div class="form-group">
    <label asp-for="Email" for="Email" class="col-form-label"></label>
    <input asp-for="Email" class="form-control" readonly />
</div>
