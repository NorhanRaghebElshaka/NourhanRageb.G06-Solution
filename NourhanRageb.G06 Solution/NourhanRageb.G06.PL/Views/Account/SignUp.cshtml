@model SignUpViewMoedel
@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_AuthLayout1.cshtml";
}
<div class="main-agileinfo">
	<div asp-validation-summary="All"></div>
	<div class="agileits-top">
		<form asp-action="SignUp">
			<input class="text" asp-for="UserName" placeholder="UserName" >
			<input class="text" asp-for="FristName" placeholder="Frist Name" >
			<input class="text" asp-for="LastName" placeholder="Last Name" >
			<input class="text" asp-for="Email" placeholder="Email" >
			<input class="text" asp-for="Password" placeholder="Password">
			<input class="text w3lpass" type="password" asp-for="ConfirmPassword" placeholder="Confirm Password">
			<div class="wthree-text">
				<label class="anim">
					<input type="checkbox" asp-for="IsAgree" class="checkbox" required="">
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Don't have an Account? <a asp-action="SignIn"> Login Now!</a></p>
	</div>
</div>